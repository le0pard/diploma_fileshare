<h2><%=t("admin.user.title")%></h2>

<table class="admin">
  <tr>
    <th><%=t("admin.user.attributes.id")%></th>
    <th><%=t("admin.user.attributes.email")%></th>
    <th><%=t("admin.user.attributes.created_at")%></th>
    <th><%=t("admin.user.attributes.updated_at")%></th>
    <th></th>
  </tr>
  <%= render :partial => 'user', :collection => @users %>
</table>

<div class='digg_pagination'>
  <%= will_paginate @users %>
</div>

<%= content_for :left_panel do %>
  <%= render :partial => 'left_panel' %>
<% end %>

<%= content_for :right_panel do %>
<% end %>
